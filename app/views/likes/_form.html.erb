<% if @article %>
  <%= form_for([@article, @article.likes.build], :html => {class:"form-horizontal"}) do |f| %>
    <%= f.submit 'Like Post' , class: "btn btn-default like-btn"%>
  <% end %>
<% else %>
  <%= form_for([@survey, @survey.likes.build], :html => {class:"form-horizontal"}) do |f| %>
    <%= f.submit 'Like Post', class: "btn btn-default like-btn"%>
  <% end %>
<% end %>
